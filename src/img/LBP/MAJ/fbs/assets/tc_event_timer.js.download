function tc_events_clic_timer(tc_elt,tc_id_event,tc_array_events){
	tc_events_8(tc_elt,tc_id_event,tc_array_events);
	var xtobj=null;
	if(tc_elt.nodeName!='A'){
		var xelp=tc_elt.parentNode;
		while(xelp){
			if(xelp.nodeName=='A'){
				xtobj=xelp;
				break;
			}
			xelp=xelp.parentNode;
		}
	}else{
		xtobj=tc_elt;
	}
	if(xtobj){
		if(xtobj.className !=='collapse__toggle'){
			xtobj.target=xtobj.target||'_self';
			if(xtobj.target.toLowerCase()=='_self'){
				setTimeout('self.location.href="'+xtobj.href+'"',500);
				return false;
			}else if(xtobj.target.toLowerCase()=='_top'){
				setTimeout('top.location.href="'+xtobj.href+'"',500);
				return false;
			}else if(xtobj.target.toLowerCase()=='_parent'){
				setTimeout('parent.location.href="'+xtobj.href+'"',500);
				return false;
			}
		}
		
		
	}
	xtkey=false;
	return true;
}